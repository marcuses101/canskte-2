{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"module": "ESNext",
		"moduleResolution": "Node",
		"target": "ESNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"types": ["bun-types"]
	},
	"include": ["./*.ts"]
}
